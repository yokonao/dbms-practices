    "@typescript-eslint/parser": "^5.19.0",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "prettier-config-freee": "1.1.0",
